{
  "name": "poko-website-builder",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "description": "A low cost, high power website builder you trully own",
  "homepage": "https://github.com/m4rrc0/poko-website-builder#readme",
  "license": "",
  "engines": {
    "bun": ">=1.2.22"
  },
  "scripts": {
    "build-archive": "BRANCH=$(git symbolic-ref --short HEAD) GIT_REMOTES=$(git remote -v) bun --bun run eleventy",
    "build": "bun --bun run eleventy",
    "cf-build": "bun i && bun run build",
    "build:gh-pages": "bun --bun run eleventy",
    "dev": "bun --bun scripts/dev.ts",
    "start": "bun --bun run eleventy --serve",
    "watch": "bun --bun run eleventy --watch",
    "serve": "bun --bun eleventy-dev-server --dir=dist",
    "clean": "rm -rf dist",
    "clean:modules": "rm -rf dist node_modules"
  },
  "keywords": [
    "website-builder",
    "cms",
    "headless-cms",
    "eleventy",
    "11ty",
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/m4rrc0/poko-website-builder.git"
  },
  "bugs": {
    "url": "https://github.com/m4rrc0/poko-website-builder/issues"
  },
  "author": {
    "name": "Marc CoÃ«t",
    "url": "https://www.m4rr.co/"
  },
  "dependencies": {
    "@sveltia/cms": "0.110.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-img": "^6.0.3",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@code-blocks/charts": "^0.1.9",
    "@code-blocks/eleventy-plugin": "^0.1.8",
    "@mdit/plugin-container": "^0.22.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@tabler/icons": "^3.34.0",
    "@types/bun": "latest",
    "bun": "^1.3.7",
    "chalk": "^5.4.1",
    "dayjs": "^1.11.13",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.7",
    "eleventy-plugin-icons": "^4.5.3",
    "eleventy-plugin-robotstxt": "^1.0.2",
    "fast-glob": "^3.3.3",
